mixin cube()
  .cube
    - sides = 6
    while sides--
      - className = sides==5?'front': ''
      - className = sides==0?'bottom': className
      - className = sides==3?'back': className
      - className = sides==4?'right': className
      - className = sides==2?'left': className
      - className = sides==1?'top': className
      .side(class=className)

  
#wall.wall
  - n = 25 
  while n--
    +cube
  .nav
